{
  "expo": {
    "name": "MyTools",
    "slug": "tool-organizer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Mytools_LOGO.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Mytools_LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.organizerapp.mytools",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.organizerapp.mytools",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Mytools_LOGO.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "react-native-adapty"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eab7d3a9-3db5-4553-b6bb-ee91a09e9a31"
      }
    }
  }
}
